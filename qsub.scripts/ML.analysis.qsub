conda activate Aim_3

# Set study
study=lupus_Chun # 88 files
# study=lupus_Chun/disease_state # 88 files

# Set working directory
cd autoimmune.datasets/$study

# Read in files
file=$(sed -n ${SGE_TASK_ID}p exp.matrix/file.list.txt)

echo $file

echo split files and select features
python3 /directflow/SCCGGroupShare/projects/lacgra/PhD/Python/Aim_3/Boruta.py $file $TMPDIR $SGE_TASK_ID
echo Run Logit
python3 /directflow/SCCGGroupShare/projects/lacgra/PhD/Python/Aim_3/logit.regression.py $file $TMPDIR $SGE_TASK_ID
echo Run RF
python3 /directflow/SCCGGroupShare/projects/lacgra/PhD/Python/Aim_3/random.forest.py $file $TMPDIR $SGE_TASK_ID
echo Run SVM
python3 /directflow/SCCGGroupShare/projects/lacgra/PhD/Python/Aim_3/support.vector.machine.py $file $TMPDIR $SGE_TASK_ID
echo Run GBM
python3 /directflow/SCCGGroupShare/projects/lacgra/PhD/Python/Aim_3/gradient.boosting.machine.py $file $TMPDIR $SGE_TASK_ID
echo Run MLP
python3 /directflow/SCCGGroupShare/projects/lacgra/PhD/Python/Aim_3/multilayer.perceptron.py $file $TMPDIR $SGE_TASK_ID

echo voting classifier
python3 /directflow/SCCGGroupShare/projects/lacgra/PhD/Python/Aim_3/voting.classifier.py $file $TMPDIR $SGE_TASK_ID